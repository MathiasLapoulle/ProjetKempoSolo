{
  "name": "kenpo-backend",
  "type": "module",
  "scripts": {
    "cli": "node --no-warnings=ExperimentalWarning --experimental-strip-types --experimental-transform-types",
    "watch": "pnpm cli --watch --",
    "dev": "pnpm watch src/main.ts",
    "start": "npm run start:prod",
    "start:prod": "node --no-warnings=ExperimentalWarning --experimental-strip-types src/main.ts",
    "build": "npm run migration:up",
    "create-admin": "pnpm cli src/createAdmin.ts",
    "migration:create": "npx tsx node_modules/@mikro-orm/cli/esm.js migration:create --config=src/mikro-orm.config.ts",
    "migration:up": "npx tsx node_modules/@mikro-orm/cli/esm.js migration:up --config=src/mikro-orm.config.ts",
    "migration:down": "npx tsx node_modules/@mikro-orm/cli/esm.js migration:down --config=src/mikro-orm.config.ts",
    "migration:list": "npx tsx node_modules/@mikro-orm/cli/esm.js migration:list --config=src/mikro-orm.config.ts",
    "migration:pending": "npx tsx node_modules/@mikro-orm/cli/esm.js migration:pending --config=src/mikro-orm.config.ts",
    "migration:fresh": "npx tsx node_modules/@mikro-orm/cli/esm.js migration:fresh --config=src/mikro-orm.config.ts",
    "schema:create": "npx tsx node_modules/@mikro-orm/cli/esm.js schema:create --config=src/mikro-orm.config.ts",
    "schema:drop": "npx tsx node_modules/@mikro-orm/cli/esm.js schema:drop --config=src/mikro-orm.config.ts",
    "schema:fresh": "npx tsx node_modules/@mikro-orm/cli/esm.js schema:fresh --config=src/mikro-orm.config.ts",
    "seeder:run": "npx tsx node_modules/@mikro-orm/cli/esm.js seeder:run --config=src/mikro-orm.config.ts",
    "init-db": "pnpm cli src/initDb.ts",
    "create-user-table": "pnpm cli src/createUserTable.ts",
    "apply-new-migrations": "pnpm cli src/applyNewMigrations.ts",
    "update-user-grade": "pnpm cli src/updateUserWithGrade.ts",
    "add-grade-column": "pnpm cli src/addGradeColumn.ts",
    "create-tournament-registration": "pnpm cli src/createTournamentRegistrationTable.ts",
    "test-tournament-registration": "pnpm cli src/testTournamentRegistration.ts",
    "check-database": "pnpm cli src/checkDatabase.ts",
    "check-and-create-table": "pnpm cli src/checkAndCreateTable.ts",
    "create-table-direct": "node src/createTableWithMysql2.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.0",
    "@hono/zod-openapi": "^0.19.2",
    "@mikro-orm/core": "^6.4.11",
    "@mikro-orm/migrations": "^6.4.11",
    "@mikro-orm/mysql": "^6.4.11",
    "@mikro-orm/seeder": "^6.4.11",
    "@scalar/hono-api-reference": "^0.7.4",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.5.0",
    "hono": "^4.7.5",
    "jsonwebtoken": "^9.0.2",
    "kenpo-backend": "file:",
    "mysql2": "^3.14.1",
    "node-mailjet": "^6.0.8",
    "pnpm": "^10.13.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.4.11",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@10.7.0+sha512.6b865ad4b62a1d9842b61d674a393903b871d9244954f652b8842c2b553c72176b278f64c463e52d40fff8aba385c235c8c9ecf5cc7de4fd78b8bb6d49633ab6"
}
